html
  body
    div.container
      div.logo
        img(src="images/Group.svg" class="logo__image" alt='Toxin')
        img(src="images/TOXIN.svg" class="logo__name" alt='Toxin')
      nav.nav
        - 
          var navArray = [
            {
                title: 'О нас',
                url: '/'
            },
            {
                title: 'Услуги',
                url: '/',
                icon: 'expand_more'
            },
            //- &#xE5CF;
            {
                title: 'Вакансии',
                url: '/'
            },
            {
                title: 'Новости',
                url: '/'
            },
            {
                title: 'Соглашения',
                url: '/',
                icon: 'expand_more'
            }
          ]
        ul.nav-list
          each item in navArray
            li.nav-list__item
              a(href=item.url)
                p= item.title 
              span.material-icons= item.icon 
                   

      div.user-button
