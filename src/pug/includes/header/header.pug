meta(charset="UTF-8")
style
  include header.css
body
  div.container
    div.logo
      img.logo__image
    nav.nav
      - 
        var navArray = [
          {
              title: 'О нас',
              url: '/'
          },
          {
              title: 'Услуги',
              url: '/'
          },
          {
              title: 'Вакансии',
              url: '/'
          },
          {
              title: 'Новости',
              url: '/'
          },
          {
              title: 'Соглашения',
              url: '/'
          }
        ]
      ul.nav-list
        each item in navArray
          li.nav-list__item
            a(href=item.url)= item.title
    div.user-button
      div.login
        button.login__button
          p ВОЙТИ
      div.registation
        button.registation__button
          p ЗАРЕГИСТРИРОВАТЬСЯ
